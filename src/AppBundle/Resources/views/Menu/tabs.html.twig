{% extends 'knp_menu.html.twig' %}

{% block root %}
    <div class="row">
        <div class="col-md-12">
            <div class="tabbable">
                <ul class="nav nav-tabs">
                    {{ block('children') }}
                </ul>
            </div>
        </div>
    </div>
{% endblock %}

{% block label %}
    {% spaceless %}
        {% if item.attribute('icon') %}<i class="fa fa-{{ item.attribute('icon') }}" aria-hidden="true"></i>{% endif %}
        {{ parent() }}
    {% endspaceless %}
{% endblock %}
